
.container{
  display: flex;
  justify-content: center;
  align-items: center;
}
.title{
  width: 100%;
  height: 30px;
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
  font-size: 14px;
  background-color: #E4E4E4;
}
.title>text{
  position: relative;
  padding-left: 10px;
}
.title>view{
  width: 80px;
  height: 25px;
  color: #66a2ff;
  position: relative;
  left: 170px;
}
.set{
  width: 100%;
  display: flex;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
}
.set_box{
  width: 95%;
  height: 50px;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
}
.set_line{
  width: 95%;
  height: 50px;
  font-size: 13px;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #E5E5E5;
}
.set_text{
  display: flex;
  width: 50%;
  height: 30px;
  font-size: 13px;
  flex-wrap: nowrap;
  align-items: center;
}
.set_input{
  width: 50%;
  height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.set_input>input{
  width: 80%;
  height: 30px;
  display: flex;
  border: 1px solid #E5E5E5;
}
.center{
    text-align: center;
}
.set_kong{
  width: 85%;
  height: 30px;
  font-size: 14px;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
}
.set_btn{
  width: 48%;
  height: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.notes{
  width: 100%;
  height: 40px;
  font-size: 12px;
  color: #bebebe;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #E5E5E5;
}
.set_box_line{
   width: 95%;
  height: 50px;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #E5E5E5;
}
.set_left{
  width: 92%;
  height: 30px;
  display: flex;
  align-items: center;
  font-size: 13px;
}
.box_input{
  width: 30%;
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.box_input>input{
  width: 100%;
  height: 30px;
  border: 1px solid #E5E5E5;
}
.box_line{
  width: 20%;
  height: 50px;
  display: flex;
  justify-content: center;
}
.box_line>view{
  width: 70%;
  height: 25px;
  border-bottom: 1px solid black;
}
.box_l{
  width: 50%;
  height: 50px;
}
.box_l>input{
  width: 80%;
  height: 45px;
  display: flex;
  flex-direction: column-reverse;
  border-bottom: 1px solid #bebebe;
  font-size: 12px;
}
.set_b{
  width: 40%;
  height: 50px;
  display: flex;
  align-items: center;
}
.set_i{
  width: 60%;
  height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.set_i>input{
  width: 80%;
  height: 30px;
  display: flex;
  border: 1px solid #E5E5E5;
}
.title_left{
  width: 100%;
  height: 50px;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 15px;
}
.set_check{
  width: 95%;
  height: 35px;
  display: flex;
  flex-wrap: nowrap;
}
.check_text{
  width: 40%;
  height: 35px;
  display: flex;
  align-items: center;
  font-size: 14px;
  padding-left: 10px;
}
.check_box{
  width: 30%;
  height: 35px;
  display: flex;
  align-items: center;
  font-size: 14px;
}
.set_table{
  width: 100%;
  height: 35px;
  display: flex;
  align-items: center;
  font-size: 14px;
  padding-left: 15px;
}
.table_box{
  width: 100%;
  height: 150px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table {
  width: 90%;
  height: 150px;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 1px solid black;
}