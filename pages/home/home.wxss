/* pages/shedlist/shedlist.wxss */
page{
  width: 100%;
  height: 100%;
}
.continer { 
 width: 100%;
 height: 100%;
 display: flex; 
 flex-direction: column; 
 padding: 0; 
 box-sizing: border-box; 
 background-color: #e4e4e4;
} 
.top{
  width: 100%;
  height: 100px;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #66a2ff;
}
.icon{
  width: 40%;
  height: 100%;
  display: flex;
  color: white;
  font-size: 14px;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.icon>image{
  width: 45px;
  height: 45px;
}
.content{
  width: 100%;
  /* height: 150px; */
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  margin-top: 5px;
}
.box{
  width: 32.5%;
  height: 70px;
  margin:0 0 1px 1px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 12px;
  flex-direction: column;
  background-color: white;
}
.icon-tianjia{
  font-size: 30px;
  color:#0099FF;
}
.icon-msnui-alarm-inverse{
  font-size: 30px;
  color: rgb(252, 69, 24);
}
.icon-shebei{
  font-size: 30px;
  color: #33CC99;
}
.icon-jibenxinxi{
  font-size:30px;
   color: #83b6f8;
}
.icon-Management{
  font-size: 26px;
  color: #0099FF;
}
.icon-xiugai{
  font-size: 25px;
  color: #FF6666;
}
.icon-guanyuwomen1{
  font-size: 30px;
  color: #ffcc00;
}
.btn{
  width: 100%;
  height: 40px;
  margin-top: 10px;
}
.btn_t{
  width: 80%;
  height: 100%;
  font-size: 15px;
  color: #333333;
  border-radius: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
}