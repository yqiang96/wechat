.container{
  background-color: #E4E4E4;
}
.up{
  display: flex;
  width: 100%;
  height: 180px;
  flex-wrap: wrap
}
.head{
  display: flex;
  width: 100%;
  height: 100px;
}
.head>image{
  width: 100%;
  height: 100px;
  z-index: 0;
  position: fixed;
}
.head_top{
  position: fixed;
  width: 100%;
  height: 100px;
}
.top_a{
  display: flex;
  width: 90%;
  height: 20%;
  margin: 10px 0 0 10px;
}
.top_a>text{
  font-size: 13px;
  color: white;
}
.icon-2{
  padding-top: 3px;
  font-size: 25px;
}
.text_a{
  margin-left: 20px;
}
.top_b{
  width: 100%;
  height: 70%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top_b>.iconfont{
  font-size: 23px;
  color: white;
  padding-bottom: 10px;
}
.top_b>.text{
  font-size: 15px;
  color: white;
  padding-bottom: 10px;
}
.top_b>.text_b{
   width: 70px;
}
.center{
  width: 100%;
  height: 80px;
  display: flex;
  flex-direction: column;
  background-color: #E4E4E4;
}
.alarm{
  width: 100%;
  height: 65px;
  display: flex;
  flex-wrap: nowrap;
  margin: 7px 0;
  align-items: center;
  background-color: white;
}
.alarm_icon{
  width: 20%;
  height: 70%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-right:2px solid #BCBCBC;
}
.alarm_icon>image{
  width: 35px;
  height: 35px;
}
/* .icon-laba{
  font-size: 25px;
  color: #66a2ff;
} */
.alarm_text{
  width: 55%;
  height: 70%;
  margin-left: 10px;
}
.text1{
  display: flex;
  width: 100%;
  height: 20px;
  font-size: 12px;
}
.text2{
  display: flex;
  width: 100%;
  height: 20px;
  font-size: 12px;
  margin-top: 10px;
}
.alarm_time{
  width: 18%;
  height: 70%;
}
.time_1{
  display: flex;
  width: 100%;
  height: 20px;
  font-size: 12px;
  color: #BCBCBC;
}
.time_2{
  display: flex;
  width: 100%;
  height: 20px;
  font-size: 12px;
  margin-top: 10px;
  color: #BCBCBC;
}
.right_icon{
  width: 5%;
  height: 100%;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
}
.icon-youjiantou1{
  font-size: 25px;
  color: #BCBCBC
}
.down{
  width: 100%;
  display: flex;
  justify-content: center;
  background-color: white;
  flex-wrap: wrap;
}
.text_d{
  width: 90%;
  height: 25px;
  color: #5E5E5E;
  margin-top: 3px;
  font-size: 16px;
}
.box{
  width: 95%;
  height: 95px;
  border: 1px solid #D7D7D7;
  display: flex;
  border-radius: 5px;
  box-shadow: -1px 2px 2px #C9C9C9;
  flex-wrap: wrap;
  margin-bottom: 10px;
}
.one{
  display: flex;
  width: 100%;
  height: 70%;
  flex-wrap: nowrap;
  align-items: center;
}
.one_icon{
  width: 61px;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.icon-dapeng{
  font-size: 40px;
  color: #66a2ff;
}
.one_text{
  display: flex;
  width: 70%;
  height: 50px;
  align-items: center;
  flex-wrap: wrap;
}
.one_1{
  width: 100%;
  height: 21px;
  font-size: 14px;
}
.one_2{
  width: 100%;
  height: 21px;
  font-size: 12px;
  color: #BCBCBC;
}
.one_image{
  width: 40px;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
}
.icon-zaixian{
  display: flex;
  width: 30px;
  height: 25px;
  font-size: 25px;
  color: #66a2ff;
  padding-top: 15px;
}
.icon-zhuangtailixian{
  display: flex;
  width: 30px;
  height: 25px;
  font-size: 25px;
  color: #BCBCBC;
  padding-top: 15px;
}
.one_image>.text{
  display: flex;
  width: 30px;
  height: 23px;
  font-size: 12px;
  color: #BCBCBC;
}
.tow{
  width: 100%;
  height: 30%;
  border-top: 1px solid #D7D7D7;
  display: flex;
  align-items: center;
}
.icon_box>.iconfont{
  font-size: 18px;
  color: #66a2ff;
}
.scroll-t{
  background-color: white
}
.tow_1{
  margin-left: 3px; 
  font-size: 13px;
  color: #66a2ff;
}
.tow_2{
  margin-left: 3px;
  font-size: 13px;
  color: #66a2ff;
}
.icon_box{
  width: 50%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}