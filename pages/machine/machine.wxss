/* pages/machine/machine.wxss */
.container{
  justify-content: center;
  align-items: center;
}
.centant{
  width: 90%;
  /* height: 110px; */
  border: 1px solid #bebebe;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  border-radius: 10px;
  font-size: 15px;
  margin-top: 10px;
}
.centant_box{
  width: 97%;
  height: 30%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 5px;
  font-size: 13px;
}
.centant_b{
  width: 97%;
  height: 30%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 5px;
  background-color: #bebebe;
  border-top-left-radius:5px;
  border-top-right-radius:5px;  
}
.box_l{
  width: 30%;
  height: 30px;
  display: flex;
  align-items: center;
}
.box_r{
  width: 70%;
  height: 30px;
  display: flex;
  align-items: center;
  font-size: 13px;
}
.box_left{
  width: 40%;
  height: 30px;
  display: flex;
  align-items: center;
}
.box_right{
  width: 60%;
  height: 30px;
  display: flex;
  align-items: center;
}
.center{
  width: 100%;
  height: 60%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  /* border-radius: 10px; */
  /* font-size: 15px; */
  /* margin-top: 10px; */
}