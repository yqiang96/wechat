/* pages/parameter/machine/machine.wxss */
.container{
  align-items: center;
  justify-content: center;
}
.content{
  width: 90%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  font-size: 14px;
}
.box_a{
  width: 100%;
  height: 30px;
  display: flex;
  flex-wrap: wrap;
}
.box_k{
  width: 100%;
  height: 30px;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.title_box{
  width: 100%;
  height: 30px;
  display: flex;
  align-items: center;
  padding-left: 10px;
}
.content_left{
  width: 50%;
  height: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content_right{
  width: 50%;
  height: 30px;
  display: flex;
  justify-content: center;
}
.content_right>input{
  width: 93%;
  height: 30px;
  border-bottom: 1px solid #bebebe;
}
.checkb{
  width: 50%;
  height: 30px;
  display: flex;
  flex-wrap: wrap;
}
checkbox-group>.checkboxs{
  width: 100%;
  height: 30px;
  display: flex;
}
.box_b{
  width: 100%;
  height: 30px;
  color: #bebebe;
  display: flex;
  align-items: center;
}
.content_input{
  width: 50%;
  height: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.center{
  text-align: center;
}
.box_btn{
  width: 100%;
  height: 30px;
  display: flex;
  margin-top: 20px;
}
.btn{
  width: 80%;
  height: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #66a2ff;
  border-radius: 10px;
  color: white;
}